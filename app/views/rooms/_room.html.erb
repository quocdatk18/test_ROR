<tr>
  <td><%= room_counter + 1 %></td>
  <td><%= room.name %></td>
  <td><%= room.user.user_name %></td>
  <% if current_user.id == room.user.id%>
    <td>
      <%= link_to "Show", room, class: 'btn btn-primary'%>
      <%= link_to 'Delete', room, method: :delete, data: { confirm: 'Delete this Room?'}, class: 'btn btn-danger' %>
    </td>
  <% else %>
   <td><%= link_to "Show", room, class: 'btn btn-primary'%></td>
  <% end %>
</tr>
