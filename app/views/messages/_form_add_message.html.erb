  <%= form_with url: room_messages_path(room_id: @room.id), method: :post, model: @message, id: 'form' do |f|%>
    <div>
      <div class="mb-3">
        <%= f.label :content, style: "display: block",class: 'form-label' %>
        <%= f.text_field :content,class: 'form-control', required: true %>
      </div>
  
      <%= f.submit 'Submit',id: 'button', class: 'btn btn-success w-100' %>
    </div>
  <% end %>


